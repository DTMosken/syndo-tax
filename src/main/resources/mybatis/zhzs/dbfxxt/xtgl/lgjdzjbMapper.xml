<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.syndo.project.zhzs.dbfxxt.mapper.xtgl.LgjdzjbMapper">
	
	<!-- 旅馆酒店中间表维护 -->
	
	<!-- 查询 -->
	<select id="selectList" resultType="com.syndo.project.zhzs.dbfxxt.domain.xtgl.Lgjdzjb" parameterType="com.syndo.project.zhzs.dbfxxt.domain.xtgl.Lgjdzjb">
		SELECT * FROM (
			SELECT OBJ_ID AS OBJID,QYMC,NSRMC,'税务入库' FLAG FROM TMP_LG_SW
		)
		WHERE 1=1
		<if test="nsrmc != null and nsrmc != ''"> and nsrmc like'%'||#{nsrmc}||'%'</if>
		<if test="qymc != null and qymc != ''"> and qymc like '%'||#{qymc}||'%'</if>
	</select>
	
	<!-- 查询一行 -->
	<select id="selectById" resultType="com.syndo.project.zhzs.dbfxxt.domain.xtgl.Lgjdzjb" parameterType="com.syndo.project.zhzs.dbfxxt.domain.xtgl.Lgjdzjb">
		SELECT OBJ_ID AS OBJID,NSRMC,QYMC,'税务入库' FLAG FROM TMP_LG_SW WHERE OBJ_ID = #{objId}
	</select>
	
	<!-- 插入 -->	
	<insert id="insert" parameterType="com.syndo.project.zhzs.dbfxxt.domain.xtgl.Lgjdzjb">
		INSERT INTO TMP_LG_SW(QYMC,NSRMC) VALUES(#{qymc},#{nsrmc})
	</insert>
	
	<!-- 修改 -->
	<update id="update" parameterType="com.syndo.project.zhzs.dbfxxt.domain.xtgl.Lgjdzjb">
		UPDATE TMP_LG_SW
			<set>
				nsrmc=#{nsrmc},qymc = #{qymc}
			</set>
		WHERE OBJ_ID = #{objId}
	</update>
	
	<!-- 删除 -->
	<delete id="deleteById" parameterType="String">
        DELETE FROM TMP_LG_SW WHERE OBJ_ID = #{objId}
    </delete>
	<!-- 批量删除 -->
    <delete id="deleteByIds" parameterType="String">
       DELETE FROM TMP_LG_SW WHERE OBJ_ID IN 
        <foreach item="objId" collection="array" open="(" separator="," close=")">
            #{objId}
        </foreach>
    </delete>
</mapper>